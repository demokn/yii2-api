{
    "name": "demokn/yii2-api",
    "description": "使用 yii2 开发 api 接口时用的扩展, 包含常用的输入验证, 格式化响应, ActiveRecord基类, 日志流Target等",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "demokn",
            "email": "demo.knyang@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.15",
        "illuminate/support": "^6.2",
        "illuminate/validation": "^6.2",
        "spatie/fractalistic": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "demokn\\api\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
